<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="codingbo.viewstudy">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <!--<uses-permission android:name="android.permission.SYS"/>-->

    <!--<uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>-->
    <!--<uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW"/>-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ThumbUpActivity"/>
        <activity android:name=".View3dActivity"/>
        <activity android:name=".touchConflict.LayoutActivity"/>
        <activity android:name=".touchConflict.ScrollActivity"/>
        <activity android:name=".FloatViewActivity"/>
        <activity android:name=".FloatContainerActivity"/>
        <activity android:name=".ShowMoreActivity"/>

        <activity android:name=".PhotoLayoutActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <!--<category android:name="android.intent.category.LAUNCHER"/>-->
            </intent-filter>
        </activity>

        <activity android:name=".MyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <!--<category android:name="android.intent.category.LAUNCHER"/>-->
            </intent-filter>
        </activity>


        <service android:name=".windowFloat.FloatService"/>
    </application>

</manifest>